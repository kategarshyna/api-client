{
  "name": "torann/api-client",
  "description": "A reusable base API client for use with remote services.",
  "keywords": ["laravel","api","curl","api client","base64 image"],
  "license": "BSD 2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "daniel@lyften.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": ">5.0 <6.0",
    "illuminate/pagination": ">5.0 <6.0",
    "symfony/http-kernel": ">2.0 <5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "BaseApiClient\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
